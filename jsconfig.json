{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@database": ["utils/database/db.js"],
      "@data": ["utils/data/data.js"],
      "@models/*": ["models/*"],
      "@utils/*": ["utils/*"]
    },
    "target": "es6",
    "moduleResolution": "node"
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
